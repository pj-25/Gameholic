<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameholic.gameControlPanel.res.layouts.components.ChatWindow">
   <children>
      <ScrollPane fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
      <HBox disable="true" spacing="5.0">
         <children>
            <TextField prefHeight="26.0" prefWidth="109.0" promptText="To..." />
            <TextField prefHeight="26.0" prefWidth="187.0" promptText="Enter your message" />
            <Button mnemonicParsing="false" text="Send" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
